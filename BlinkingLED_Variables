//Blinking_LED - Using variables
//turn on the LED for half a second,then off for half a second,repeatedly
int ledPin = 9;//the number of the LED pin
int waitTimeS=500; //short wait time
int waitTimeL=500; //long wait time
void setup()
{
pinMode(ledPin,OUTPUT);//initialize the digital pin as an output
}
void loop()
{
digitalWrite(ledPin,HIGH);//turn the LED on
delay(waitTimeS);               //wait for half a second
digitalWrite(ledPin,LOW); //turn the LED off
delay(waitTimeL);               //wait for half a second
}

